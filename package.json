{
  "name": "react-css-spinners",
  "version": "1.0.0",
  "description": "CSS-only spinners for React",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint $PWD/'**/*.js' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --findRelatedTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex996/react-css-spinners.git"
  },
  "keywords": [
    "react",
    "css",
    "spinners"
  ],
  "author": "Alex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex996/react-css-spinners/issues"
  },
  "homepage": "https://github.com/alex996/react-css-spinners#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^26.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.1",
    "jest": "^26.1.0",
    "lint-staged": "^8.1.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-terser": "^3.0.0",
    "serve": "^10.1.1"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/mocks/styles.js"
    }
  },
  "dependencies": {
    "react": "^16.13.1"
  }
}
